{
  "name": "react-idle-timer-babel6",
  "version": "1.2.10",
  "description": "Activity detection for React.js",
  "main": "lib/index.js",
  "browser": "lib/browser.js",
  "files": [
    "lib",
    "examples"
  ],
  "scripts": {
    "clean": "rimraf build && rimraf lib && rimraf examples/App.js && rimraf examples/init.js && rimraf examples/server.js",
    "build": " babel src_examples -d examples && webpack --config webpack.prod.config.js --progress --profile --colors",
    "build:test": "webpack --config webpack.prod.config.js --progress --profile --colors",
    "_build": "babel src -d lib && babel src_examples -d examples",
    "_build:browser": "webpack --config webpack.prod.config.js --progress --profile --colors",
    "rebuild": "npm run clean && npm run build",
    "rebuild:test": "set NODE_ENV=production&& rimraf libtest && npm run build:test",
    "_rebuild:prod": "NODE_ENV=production npm run clean && npm run build && npm run build:browser",
    "_rebuild:prod:windows": "set NODE_ENV=production&& npm run clean && npm run build && npm run build:browser",
    "start": "node examples/server",
    "test": "npm test",
    "_prepublish": "in-publish && npm shrinkwrap || not-in-publish",
    "_postpublish": "rimraf npm-shrinkwrap.json",
    "package": "NODE_ENV=production npm run rebuild && npm version patch && npm publish --tag latest",
    "package:windows": "set NODE_ENV=production&& npm run rebuild && npm version patch && npm publish --tag latest",
    "_package": "npm run rebuild:prod && npm version patch && npm publish --tag latest",
    "_package:windows": "npm run rebuild:prod:windows && npm version patch && npm publish --tag latest"
  },
  "keywords": [
    "react",
    "idle",
    "idletimer",
    "timer",
    "activity",
    "active"
  ],
  "author": "Randy Lebeau",
  "contributors": [
    "Cole Chamberlain <cole.chamberlain@gmail.com> (https://github.com/cchamberlain)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/supremetechnopriest/react-idle-timer.git"
  },
  "dependencies": {
    "in-publish": "^2.0.0",
    "moment": "^2.10.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "del": "^1.2.0",
    "es3ify": "^0.1.4",
    "es3ify-loader": "^0.1.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-eslint": "^0.14.0",
    "gulp-shell": "^0.4.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "jest-cli": "^0.4.13",
    "lodash.bindall": "^3.1.0",
    "lodash.noop": "^3.0.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
